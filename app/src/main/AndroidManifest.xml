<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ngeeann.musicplayer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ngeeann.musicplayer.Start">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity android:name="com.ngeeann.musicplayer.NowPlayingActivity" />
        <activity android:name="com.ngeeann.musicplayer.AlbumActivity" />
        <activity android:name="com.ngeeann.musicplayer.SongActivity" />
        <activity android:name="com.ngeeann.musicplayer.MainActivity"/>
    </application>

</manifest>